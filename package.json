{
  "name": "action-testng-report",
  "version": "1.0.0",
  "description": "GitHub Action to generate TestNG report",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
  "test": "jest --config jest.config.mjs",
    "lint": "eslint src __tests__ --ext .ts",
    "lint:fix": "eslint src __tests__ --ext .ts --fix",
    "bundle": "ncc build src/index.ts -o dist"
  },
  "keywords": [
    "github-action",
    "testng",
    "report",
    "ci"
  ],
  "author": "Anoop Garlapati and RuneKit Contributors",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "fast-xml-parser": "^4.3.5",
    "glob": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.5"
  }
}